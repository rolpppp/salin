{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/src/(.*)",
      "dest": "/client/public/src/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/public/$1"
    }
  ],
  "outputDirectory": "client/public"
}